GEM5GPU_GCC_PATH=/usr/local/gcc-4.8.0
GEM5GPU_CUDA_PATH=/usr/local/cuda-3.2
GEM5GPU_LIBRARYS_PATH=/usr/local/librarys
GEM5GPU_PATH=$PWD
OPENCLORCUDA=OPENCL

export PATH=$GEM5GPU_GCC_PATH/bin:$GEM5GPU_CUDA_PATH/cuda/bin:$GEM5GPU_LIBRARYS_PATH/bin:$PATH
export LD_LIBRARY_PATH=$GEM5GPU_GCC_PATH/lib64:$GEM5GPU_LIBRARYS_PATH/lib:$LD_LIBRARY_PATH
export CUDAHOME=$GEM5GPU_CUDA_PATH/cuda
export CUDA_INSTALL_PATH=$GEM5GPU_CUDA_PATH/cuda
export NVIDIA_CUDA_SDK_LOCATION=$GEM5GPU_CUDA_PATH/NVIDIA_GPU_Computing_SDK/C
export NVOPENCL_LIBDIR=/usr/lib64
export GEM5GPU_ROOT=$GEM5GPU_PATH
export GEM5GPU_CUDA_OR_OPENCL=$OPENCLORCUDA
